      <!--END PAGE CONTENT -->
      <!-- RIGHT STRIP  SECTION -->
      <div id="right">
        <div class="well well-small">
          <button class="btn btn-block"> <b>Rooms by Type</b> </button>
          <ul class="list-unstyled">
            <% room_types.each do |room_type| %>
              <li><span class="col-lg-8"><%= room_type.room_type %></span> <button class="btn btn-default btn-circle" type="button" ><span><%= room_type.rooms.count %></span></button></li>
            <% end %>
          </ul>
        </div>

        <div class="well well-small">
          <button class="btn btn-block"> <b>Rates By Room</b> </button>

          <div class="panel panel-info">
            <div class="panel-body">
              <div class="panel-group" id="accordion">

                <% room_rates.each do |room, room_rate| %>
                  <div class="panel panel-default">
                    <div class="panel-heading">
                      <h4 class="panel-title">
                        <a data-toggle="collapse" data-parent="#accordion" href="#<%= room.room_id %>" class="collapsed"><%= room.name %></a>
                      </h4>
                    </div>
                    <div id="<%= room.room_id %>" class="panel-collapse collapse" style="height: 0px;">
                      <div class="panel-body">
                        <% unless room_rate.blank? %>
                          <b><%= number_to_currency(room_rate.rate, :unit => 'MK') %></b> created on <b><%= room_rate.created_at.to_date.strftime("%d/%b/%Y") %></b>
                        <% else %>
                          Not set
                        <% end %>

                      </div>
                    </div>
                  </div>
                <% end %>

              </div>
            </div>
          </div>

        </div>

        <div class="well well-small">
          <button class="btn btn-block"> <b>Users</b> </button>
          <div class="panel panel-info">
            <div class="panel-body">
              <div class="panel-group" id="accordion">

                <% available_users.each do |user| %>
                  <div class="panel panel-default">
                    <div class="panel-heading">
                      <h4 class="panel-title">
                        <a data-toggle="collapse" data-parent="#accordion" href="#<%= user.username %>" class="collapsed"><%= user.username %></a>
                      </h4>
                    </div>
                    <div id="<%= user.username %>" class="panel-collapse collapse" style="height: 0px;">
                      <div class="panel-body">
                        <b><%= user.first_name.to_s + " " + user.last_name.to_s %></b> member since <b><%= user.created_at.to_date.strftime("%d/%b/%Y") %></b>
                      </div>
                    </div>
                  </div>
                <% end %>

              </div>
            </div>
          </div>
        </div>
      </div>
