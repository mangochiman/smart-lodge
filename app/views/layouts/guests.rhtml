    <script type="text/javascript">
      $(function () { formInit(); });
      $('a.media').media({width:'100%', height:400});

      window.onload = function () {
        jQuery('.faculty-div').hide();
        setUserTable();
      }

      function setUserTable() {
        var panels = $('.user-infos');
        var panelsButton = $('.dropdown-user');
        panels.hide();

        //Click dropdown
        panelsButton.click(function() {
          //get data-for attribute
          var dataFor = $(this).attr('data-for');
          var idFor = $(dataFor);

          //current button
          var currentButton = $(this);
          idFor.slideToggle(400, function() {
            //Completed slidetoggle
            if(idFor.is(':visible'))
            {
              currentButton.html('<i class="glyphicon glyphicon-chevron-up text-muted"></i>');
            }
            else
            {
              currentButton.html('<i class="glyphicon glyphicon-chevron-down text-muted"></i>');
            }
          })
        });


        $('[data-toggle="tooltip"]').tooltip();

      };

      function browseByCategory(categoryID){
        window.location = "/browse_by_category/" + categoryID;
      }

      function browseByFaculty(facultyID){
        window.location = "/view_by_faculty/" + facultyID;
      }

      $(function () { formWysiwyg(); });

      function checkUncheckAll(){
        $("#select_all").change(function(){  //"select all" change
          $(".checkbox").prop('checked', $(this).prop("checked")); //change all ".checkbox" checked status
        });
      }

      checkUncheckAll();
    </script>
